<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.xwkj.shopping.domain">
	<class name="Sendee" table="sendee">
		<id name="sid" column="sid" type="string">
			<generator class="uuid"/>
		</id>
		<property name="uid" column="uid" type="string" not-null="true"/>
		<property name="sname" column="sname" type="string" not-null="true"/>
		<property name="email" column="email" type="string" not-null="true"/>
		<property name="address" column="address" type="string" not-null="true"/>
		<property name="telephone" column="telephone" type="string" not-null="true"/>
	</class>
</hibernate-mapping>