<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC 
	"-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN"
	"http://getahead.org/dwr/dwr30.dtd">
<dwr>
	<allow>
		<create creator="spring" javascript="SendeeManager"> 
			<param name="beanName" value="sendeeManager"/>
		</create>
		<create creator="spring" javascript="AdminManager"> 
			<param name="beanName" value="adminManager"/>
		</create>
		<create creator="spring" javascript="TypeManager"> 
			<param name="beanName" value="typeManager"/>
		</create>
		<create creator="spring" javascript="CategoryManager"> 
			<param name="beanName" value="categoryManager"/>
		</create>
		<create creator="spring" javascript="GoodManager"> 
			<param name="beanName" value="goodManager"/>
		</create>
		<create creator="spring" javascript="PhotoManager"> 
			<param name="beanName" value="photoManager"/>
		</create>
		<create creator="spring" javascript="BasketManager"> 
			<param name="beanName" value="basketManager"/>
		</create>
		<create creator="spring" javascript="OrderManager"> 
			<param name="beanName" value="orderManager"/>
		</create>
		<create creator="spring" javascript="CommentManager"> 
			<param name="beanName" value="commentManager"/>
		</create>
		<create creator="spring" javascript="MailManager"> 
			<param name="beanName" value="mailManager"/>
		</create>
		<!-- 定义使用Bean转换器处理如下Java类 -->
		<convert converter="bean" match="com.xwkj.shopping.bean.IconBean"/> 
		<convert converter="bean" match="com.xwkj.shopping.bean.TypeBean"/> 
		<convert converter="bean" match="com.xwkj.shopping.bean.CategoryBean"/> 
		<convert converter="bean" match="com.xwkj.shopping.bean.PhotoBean"/> 
		<convert converter="bean" match="com.xwkj.shopping.bean.GoodBean"/> 
		<convert converter="bean" match="com.xwkj.shopping.bean.SendeeBean"/> 
		<convert converter="bean" match="com.xwkj.shopping.bean.UserBean"/> 
		<convert converter="bean" match="com.xwkj.shopping.bean.BasketBean"/> 
		<convert converter="bean" match="com.xwkj.shopping.bean.OrderBean"/> 
		<convert converter="bean" match="com.xwkj.shopping.bean.CommentBean"/> 
	</allow>
</dwr>